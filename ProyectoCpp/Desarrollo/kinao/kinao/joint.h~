#ifndef JOINT_H
#define JOINT_H

#include "kinao_global.h"

#define PI = 3.14159265358979323846

class Joint
{
private:

public:
    Joint();//Constructor del Objeto
    virtual ~Joint(void);//Destructor del Objeto

    //Determina el vector que va de P1 a P2
    virtual float length (float[], float[]);// P1x,P1y,P1z,P2x,P2y,P2z

    virtual float angle(float, float, float);//L1,L2,L3(opuesto del angulo a determinar)

};

#endif // JOINT_H
